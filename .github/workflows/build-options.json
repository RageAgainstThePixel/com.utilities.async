{
  "os": [
    "ubuntu-latest",
    "windows-latest",
    "macos-latest"
  ],
  "unity-version": [
    "2021",
    "2022",
    "6000.0",
    "6000.1",
    "6000.2"
  ],
  "build-args": [
    "-sceneList Assets/Samples/Demo/AsyncSample.unity"
  ],
  "include": [
    {
      "os": "ubuntu-latest",
      "build-target": "StandaloneLinux64"
    },
    {
      "os": "ubuntu-latest",
      "build-target": "WebGL"
    },
    {
      "os": "ubuntu-latest",
      "build-target": "Android"
    },
    {
      "os": "windows-latest",
      "build-target": "StandaloneWindows64"
    },
    {
      "os": "macos-latest",
      "build-target": "StandaloneOSX"
    },
    {
      "os": "macos-latest",
      "build-target": "iOS"
    }
  ]
}